{
  "name": "@zippytech/react-toolkit",
  "author": "Zippy Technologies <contact@zippytech.io>",
  "license": "MIT",
  "version": "1.0.54",
  "repository": {
    "type": "git",
    "url": "https://github.com/zippytech/react-toolkit.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "dev": "webpack-dev-server --progress --colors --config ./build/dev.config.js",
    "lib": "node ./scripts/lib.js",
    "lib:all": "npm run lib --module=ALL",
    "lib:style": "node ./scripts/lib-style.js",
    "lib:js": "node ./scripts/lib-js.js",
    "lib:themes": "node ./scripts/lib-themes",
    "lint": "eslint src/**/*.js",
    "build": "npm run lib:all && node ./scripts/lib-index-css.js && npm run build:package && cp README.md ./lib && cp .npmignore ./lib",
    "build:package": "node ./scripts/buildGlobalPackageJSON.js",
    "pretty": "prettier --single-quote es5 --write \"src/**/*.{js,jsx,json,css,scss}\" --fix",
    "prettystyle": "stylelint \"src/**/*.scss\" --syntax scss --fix",
    "precommit": "lint-staged",
    "release": "npm run release:patch",
    "release:patch": "npm run build && release patch && npm run registry-publish",
    "release:minor": "npm run build && release minor && npm run registry-publish",
    "release:major": "npm run build && release major && npm run registry-publish",
    "registry-publish": "npm run build:package && npm config delete @zippytech:registry && npm publish lib --access public &&  npm config set @zippytech:registry https://registry.zippytech.io/"
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "no-duplicate-selectors": null,
      "no-empty-source": null,
      "block-no-empty": null,
      "at-rule-no-unknown": null,
      "selector-combinator-space-before": null,
      "no-descending-specificity": null,
      "function-comma-space-after": null,
      "declaration-bang-space-before": null,
      "selector-pseudo-element-colon-notation": null,
      "declaration-colon-newline-after": null,
      "selector-descendant-combinator-no-non-space": null,
      "value-list-comma-newline-after": null
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "prettier --single-quote --write",
      "git add"
    ],
    "src/**/*.scss": [
      "prettier --single-quote --write",
      "stylelint --syntax scss --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@zippytech/theme-builder": "^2.0.3",
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^22.4.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chalk": "^2.3.2",
    "css-loader": "^0.23.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "faker": "^3.1.0",
    "file-loader": "^0.9.0",
    "fs-extra": "^5.0.0",
    "fs-promise": "^2.0.0",
    "github-markdown-css": "^2.4.1",
    "globby": "^6.1.0",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "jest-environment-node": "^22.4.1",
    "json-loader": "^0.5.4",
    "lint-staged": "^7.0.0",
    "mkdirp": "^0.5.1",
    "moment": "2.18.1",
    "node-sass": "^4.7.2",
    "pify": "^2.3.0",
    "prettier": "^1.11.1",
    "prop-types": "^15.6.1",
    "puppeteer": "^1.1.1",
    "raw-loader": "^0.5.1",
    "react": "16",
    "react-addons-perf": "15.6.0-rc.1",
    "react-addons-test-utils": "15.6.2",
    "react-components-markdown": "^0.2.0",
    "react-dom": "16",
    "react-hot-loader": "^1.3.0",
    "react-router": "^2.7.0",
    "react-router-dom": "^4.2.2",
    "react-syntax-highlighter": "^5.5.0",
    "react-test-renderer": "^15.3.0",
    "regenerator-runtime": "^0.11.1",
    "release": "^3.1.7",
    "rimraf": "^2.6.2",
    "sass-loader": "^4.0.0",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "snake-case": "^2.1.0",
    "style-loader": "^0.13.1",
    "stylelint": "^9.1.3",
    "stylelint-config-standard": "^18.2.0",
    "typeface-roboto": "0.0.35",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
    "moment": ">=2.18.1",
    "prop-types": ">=15.5.0",
    "react": ">=0.14.0 || >=15.0.0-0 || >=16.0.0-0",
    "react-dom": ">=0.14.0 || >=15.0.0-0 || >=16.0.0-0"
  },
  "dependencies": {
    "@zippytech/drag-helper": "^2.0.4",
    "@zippytech/has-touch": "^2.0.2",
    "@zippytech/react-class": "^4.0.6",
    "@zippytech/react-scroll-container": "^2.0.15",
    "@zippytech/region": "^3.0.8",
    "@zippytech/region-align": "^3.0.6",
    "@zippytech/uglified": "^2.0.1",
    "lodash.throttle": "^4.1.1",
    "react-lifecycles-compat": "^3.0.4",
    "tinycolor2": "^1.4.1",
    "xhr": "^2.4.0"
  },
  "keywords": [
    "react",
    "react-component",
    "front-end",
    "ui",
    "toolkit",
    "library",
    "accordion",
    "autocomplete",
    "button",
    "button-group",
    "calendar",
    "check-box",
    "combobox",
    "combo-box",
    "color-picker",
    "colorpicker",
    "dateinput",
    "date-input",
    "dropdown",
    "dropdownbutton",
    "dropdown-button",
    "loadmask",
    "maskedinput",
    "menu",
    "messagewindow",
    "message-window",
    "multiselect",
    "notification",
    "numericinput",
    "numeric-input",
    "panel",
    "progressbar",
    "progress-bar",
    "radiobuttongroup",
    "radio-button-group",
    "slider",
    "splitbutton",
    "split-button",
    "splitcontainer",
    "split-container",
    "tabpanel",
    "tab-panel",
    "toolbar",
    "tooltip",
    "treeview",
    "tree-view",
    "uploader",
    "window"
  ]
}
